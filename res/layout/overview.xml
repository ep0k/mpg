<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal">
    <fragment android:name="com.ford.openxc.mpg.HistoricalMpgChartFragment"
            android:id="@+id/historical_mpg_chart_fragment"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent" />

    <fragment android:name="com.ford.openxc.mpg.HistoricalFuelConsumptionChartFragment"
            android:id="@+id/historical_fuel_chart_fragment"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent" />

</LinearLayout>
